theme_minimal() +
theme(legend.position="right",
plot.title = element_text(size=9),
axis.title.x = element_text(size=8),
axis.title.y = element_text(size=8))
ggsave("X9_Plots/success_plot.pdf", width = 3.5, height = 2.5, unit = "in", dpi = 300)
